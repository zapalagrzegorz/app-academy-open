load '5_hanoi.rb'

h = Hanoi.new

h.move([0,1])


        h.move([0, 1])
        # 23 1 0
        h.move([0, 2])
        # 3 1 2
        h.move([1, 2])

        # 3 0 12
        h.move([0, 1])

        # 0 3 12
        h.move([2, 0])

        # 1 3 2
        h.move([2, 1])

        # 1 23 0
        h.move([0, 1])